* {
  background-color: #010D1C;
  border-color: #2e343f;
  text-color: #8ca0aa;
  spacing: 0;
  width: 512px;
}

window {
  location: center;
  anchor:  center;
}

listview {
  cycle: false;
  margin: 0 0 -1px 0;
  scrollbar: false;
}

element {
  border: 0 0 1px 0;
  padding: 16px;
}

element selected {
  background-color: #2e343f;
}

element-text, element-icon {
    background-color: inherit;
    text-color:       inherit;
}

element-icon {
    margin: 2px 13px 0 0;
}

